{
  "namespace": "rainbowpieui_module_pack",

  "module_list@rainbowpie_ui_module_list.module_list_panel": {
    "controls": [
      {
        "chest_fastpick@rainbowpie_ui_module_list.module_item": {
          "$module_icon_texture": "($rainbowpie_ui_path_home + '/icon/module')",
